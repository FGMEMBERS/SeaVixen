<?xml version="1.0"?>

<PropertyList>

	<fx>
	
	<engine>
      <name>engine-start-inside</name>
      <mode>once</mode>
      <path>Aircraft/SeaVixen/Sounds/turbine_start_in.wav</path>
      <condition>
        <and>
          <property>/controls/engines/engine[0]/starter</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        <min> 0.03 </min>
        <max> 0.07 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
        <min> 0.0 </min>
        <max> 0.9 </max>
      </pitch>
       <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
   </engine>
   
   
   <engine>
      <name>engine-start-outside</name>
      <mode>once</mode>
      <path>Aircraft/SeaVixen/Sounds/vixen-startup.wav</path>
      <condition>
<and>
 
<greater-than>  
   <property>/engines/engine[0]/rpm</property>
   <value>0.5</value>
   
  </greater-than>
  <less-than>
                <property>velocities/groundspeed-kt</property>
                <value>1</value>
            </less-than>
  <not>
   <property>/sim/current-view/internal</property>
  </not>
</and>
</condition>

      <volume>
        <property>engines/engine[0]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/n2</property>
        <factor> 0.01 </factor>
	<min> 0.0 </min>
	<max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
	
	<engine>
      <name>engine-start-outside1</name>
      <mode>once</mode>
      <path>Aircraft/SeaVixen/Sounds/vixen-startup.wav</path>
      <condition>
<and>
 
<greater-than>  
   <property>/engines/engine[1]/rpm</property>
   <value>0.5</value>
   
  </greater-than>
  <less-than>
                <property>velocities/groundspeed-kt</property>
                <value>1</value>
            </less-than>
  <not>
   <property>/sim/current-view/internal</property>
  </not>
</and>
</condition>

      <volume>
        <property>engines/engine[1]/n2</property>
        <factor> 0.09 </factor>
        <min> 0.3 </min>
        <max> 0.9 </max>
      </volume>
      <pitch>
        <property>engines/engine[1]/n2</property>
        <factor> 0.01 </factor>
	<min> 0.0 </min>
	<max> 0.9 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>
   

		<engine>
			<name>engine 1</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<property>sim/current-view/internal</property>
				<property>engines/engine/running</property>
				</and>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine/thrust-lbs</property>
				<factor>0.00003</factor>
			</volume>
		</engine>

		<engine>
			<name>engine 2</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<property>sim/current-view/internal</property>
					<property>engines/engine[1]/running</property>
				</and>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine[1]/thrust-lbs</property>
				<factor>0.00003</factor>
			</volume>
		</engine>

		<engine>
			<name>engine 1 external</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<not>
					<property>sim/current-view/internal</property>
					</not>
					<property>engines/engine/running</property>
				</and>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine/thrust-lbs</property>
				<factor>0.00012</factor>
			</volume>
		</engine>

		<engine>
			<name>engine 2</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<not>
					<property>sim/current-view/internal</property>
					</not>
					<property>engines/engine[1]/running</property>
				</and>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<volume>
				<property>engines/engine[1]/thrust-lbs</property>
				<factor>0.00012</factor>
			</volume>
		</engine>

		<engine>
			<name>external whine 1</name>
			<path>Aircraft/SeaVixen/Sounds/vixen-whine.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<property>engines/engine/running</property>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>-5.0</y>
				<z>0.0</z>
				<inner-angle>50.0</inner-angle>
				<outer-angle>180.0</outer-angle>
				<outer-gain>0.0</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine/n1</property>
				<ofset>0.7</ofset>
				<factor>.003</factor>
			</pitch>
			<volume>
				<property>engines/engine/n1</property>
				<offset>1.0</offset>
				<factor>-0.008</factor>
				<max>0.6</max>
			</volume>
		</engine>

		<engine>
			<name>external whine 2</name>
			<path>Aircraft/SeaVixen/Sounds/vixen-whine.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<property>engines/engine/running</property>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>-5.0</y>
				<z>0.0</z>
				<inner-angle>50.0</inner-angle>
				<outer-angle>180.0</outer-angle>
				<outer-gain>0.0</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[1]/n1</property>
				<ofset>0.7</ofset>
				<factor>.003</factor>
			</pitch>
			<volume>
				<property>engines/engine[1]/n1</property>
				<offset>1.0</offset>
				<factor>-0.008</factor>
				<max>0.6</max>
			</volume>
		</engine>

		<engine>
			<name>internal whine 1</name>
			<path>Sounds/whine.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<property>engines/engine/running</property>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>-5.0</y>
				<z>0.0</z>
				<inner-angle>50.0</inner-angle>
				<outer-angle>180.0</outer-angle>
				<outer-gain>0.0</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine/n1</property>
				<ofset>0.7</ofset>
				<factor>.00075</factor>
			</pitch>
			<volume>
				<max>0.6</max>
			</volume>
		</engine>

		<engine>
			<name>internal whine 2</name>
			<path>Sounds/whine.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<property>engines/engine[1]/running</property>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>-5.0</y>
				<z>0.0</z>
				<inner-angle>50.0</inner-angle>
				<outer-angle>180.0</outer-angle>
				<outer-gain>0.0</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[1]/n1</property>
				<ofset>0.7</ofset>
				<factor>.00075</factor>
			</pitch>
			<volume>
				<max>0.6</max>
			</volume>
		</engine>

		<rumble>
			<name>rumble 1</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<property>gear/gear[0]/wow</property>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>.004</factor>
			</volume>
		</rumble>

		<rumble>
			<name>rumble 2</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<property>gear/gear[1]/wow</property>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>.004</factor>
			</volume>
		</rumble>

		<rumble>
			<name>rumble 3</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<property>gear/gear[2]/wow</property>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>.004</factor>
			</volume>
		</rumble>

		<squeal>
			<name>tailskid 1</name>
			<mode>looped</mode>
			<path>Sounds/squeal.wav</path>
			<condition>
				<property>gear/gear[3]/wow</property>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>.04</factor>
			</volume>
		</squeal>


		<squeal>
			<name>tailskid 2</name>
			<mode>looped</mode>
			<path>Sounds/squeal.wav</path>
			<condition>
				<property>gear/gear[4]/wow</property>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>.04</factor>
			</volume>
		</squeal>

		<gear>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Sounds/whine.wav</path>
			<property>gear/gear[0]/position-norm</property>
			<volume>
				<offset>0.125</offset>
				<min>0.1</min>
				<max>0.9</max>
			</volume>
		</gear>

		<gear>
			<name>gear-lock</name>
			<path>Sounds/gear-lck.wav</path>
			<condition>
				<equals>
					<property>gear/gear[0]/position-norm</property>
					<value>0</value>
				</equals>
			</condition>
		</gear>

		<squeal>
			<name>squeal</name>
			<path>Sounds/squeal.wav</path>
			<condition>
				<or>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
		</squeal>

		<flap>
			<name>flap</name>
			<mode>in-transit</mode>
			<path>Sounds/whine.wav</path>
			<property>surface-positions/flap-pos-norm</property>
			<volume>
				<offset>0.125</offset>
				<min>0.1</min>
				<max>0.9</max>
			</volume>
		</flap>

		<rumble>
			<name>flap rumble</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<property>surface-positions/flap-pos-norm</property>
				<value>1</value>
			</condition>
			<volume>
				<property>position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.125</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.12</factor>
				<min>0.125</min>
				<max>1.0</max>
			</volume>
		</rumble>


		<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<property>velocities/airspeed-kt</property>
			<volume>
				<property>position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.0015</factor>
				<min>0.03</min>
				<max>0.25</max>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>1.25</offset>
			</pitch>
		</wind>

		<speedbrake>
			<name>speedbrake</name>
			<mode>in-transit</mode>
			<path>Sounds/whine.wav</path>
			<property>surface-positions/speedbrake-pos-norm</property>
			<volume>
				<offset>0.125</offset>
				<min>0.1</min>
				<max>0.9</max>
			</volume>
		</speedbrake>

		<rumble>
			<name>speedbrake rumble</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<property>surface-positions/speedbrake-pos-norm</property>
				<value>1</value>
			</condition>
			<volume>
				<property>position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.125</min>
				<max>1.0</max>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.12</factor>
				<min>0.125</min>
				<max>1.0</max>
			</volume>
		</rumble>

		<!-- Sonic Boom -->
  <sonicboom>
   <name>Sonic Boom</name>
	<mode>once</mode>
   <path>Aircraft/SeaVixen/Sounds/sonicboom.wav</path>
   <condition>
   <and>
	    <greater-than>
	     	<property>/velocities/mach</property>
	     	<value>1.0</value>
	    </greater-than>
	    <equals>
		    <property>/sim/current-view/internal</property>
		    <value>false</value>
		</equals>
	</and>
   </condition>
  </sonicboom>
  
  
   <engine>
      <name>engine</name>
      <path>Aircraft/SeaVixen/Sounds/jet-blast.wav</path>
      <mode>looped</mode>
      <condition>
        <property>engines/engine[0]/running</property>
		<not>
        	<property>/sim/current-view/internal</property>
		<property>/engines/engine/out-of-fuel</property>
     	</not>
      </condition>
      <volume>
        <property>engines/engine[0]/prop-thrust</property>
        <factor type="double">0.00013</factor>
      </volume>
       <pitch>
        <property>engines/engine[0]/n1</property>
        <factor>0.2</factor>
      </pitch>
    </engine>
	
	<engine>
      <name>engine1</name>
      <path>Aircraft/SeaVixen/Sounds/jet-blast.wav</path>
      <mode>looped</mode>
      <condition>
        <property>engines/engine[1]/running</property>
		<not>
        	<property>/sim/current-view/internal</property>
		<property>/engines/engine/out-of-fuel</property>
     	</not>
      </condition>
      <volume>
        <property>engines/engine[1]/prop-thrust</property>
        <factor type="double">0.00013</factor>
      </volume>
       <pitch>
        <property>engines/engine[0]/n1</property>
        <factor>0.2</factor>
      </pitch>
    </engine>

  
  <!-- flyby -->
		<flyby>
			<name>fly-by</name>
			<mode>looped</mode>
			<path>Aircraft/SeaVixen/Sounds/flyby.wav</path>
			<condition>
			<and>
	    <greater-than>
	     	<property>velocities/mach</property>
	     	<value>0.8</value>
	    </greater-than>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>6</value>
				</equals>
				</and>
				<not>
         	<property>/sim/current-view/internal</property>
	 	<property>/engines/engine/out-of-fuel</property>
     	</not>
		</condition>
			<!-- <volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
			-->
		</flyby>
		
		<voice>
  <name>Geforce-breath-vox</name>
  <path>Aircraft/SeaVixen/Sounds/Geforce.wav</path>
  <condition>
   <greater-than>
    <property>/accelerations/pilot/z-accel-fps_sec</property>
    <value>-161</value>
   </greater-than>   
  </condition>
<volume>
<factor>0.9</factor>
</volume>
 </voice>

<voice>
  <name>Geforce-vox</name>
  <path>Aircraft/SeaVixen/Sounds/geforce1.wav</path>
  <condition>
   <less-than>
    <property>/accelerations/pilot/z-accel-fps_sec</property>
    <value>-225</value>
   </less-than>   
  </condition>
<volume>
<factor>0.9</factor>
</volume>
 </voice>
 
  
 <crash>
  <name>intense ground contact</name>
  <mode>once</mode>
  <path>Sounds/crash.wav</path>
  <property>sim/crashed</property>
  <volume>
    <factor>1</factor>
  </volume>
  <pitch>
    <factor>1.5</factor>
  </pitch>
</crash>
		
		
	</fx>

</PropertyList>
